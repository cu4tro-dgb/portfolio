---
import Github from './icons/Github.astro'
import Instagram from './icons/Instagram.astro'
import Linkedin from './icons/Linkedin.astro'
import Dark from './icons/Dark.astro'
---

<header class="fixed w-full mt-4 z-50 mix-blend-difference backdrop-blur-sm px-2">
  <div class="relative flex container mx-auto justify-between border rounded-full py-2 px-4 bg-slate-400/10 ">
    <div class="flex gap-x-3">
      <Github class="size-6 text-white " />
      <Linkedin class="size-6 text-white" />
      <Instagram class="size-6 text-white" />
    </div>
    <div>
      <Dark class="size-6 text-white" />
    </div>
  </div>
</header>
